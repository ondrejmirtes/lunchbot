{
	"name": "ondrejmirtes/lunchbot",
	"description": "Bot for posting nearby daily lunch menus to a Slack channel",
	"license": [
		"MIT"
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "~7.0",
		"guzzlehttp/guzzle": "~6.2",
		"nette/bootstrap": "~2.4.3",
		"nette/utils": "~2.4.6",
		"symfony/console": "~3.3.0",
		"symfony/css-selector": "~3.3.0",
		"symfony/dom-crawler": "~3.3.1",
		"composer/ca-bundle": "^1.0"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "~0.9.2",
		"phpunit/phpunit": "~6.2.1"
	},
	"autoload": {
		"psr-4": {
			"Lunchbot\\": "src/"
		}
	}
}
